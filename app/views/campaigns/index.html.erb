<div id="fwrapper" class="col-lg-9 col-lg-offset-0 col-md-offset-0 col-md-9 col-sm-9 col-xs-9">
  <h1>Listing campaigns for <%= current_client.name.titlecase %></h1>
  <table class="table">
    <thead>
    <tr>
      <th>Campaign Name</th>
      <th>Campaign Status</th>
      <th></th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
    </thead>

    <tbody>
    <% @campaigns.each do |campaign| %>
        <tr>
          <td><%= campaign.name %></td>
          <td><%= campaign.active ? "Active": "Inactive" %></td>
          <td><%= link_to 'Show', campaign %></td>
          <td><%= link_to 'Edit', edit_campaign_path(campaign) %></td>
          <td><%= link_to 'Build Campaign', build_the_campaign_path(campaign) %></td>
          <td><%= link_to 'Destroy', campaign, method: :delete, data: {confirm: 'Are you sure?'} %></td>
        </tr>
    <% end %>
    </tbody>
  </table>

  <br>
  <%= link_to "New Campaign", new_campaign_path, class: "btn btn-danger btn-lg" %>
</div>

